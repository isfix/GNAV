{
    "version": 8,
    "name": "PANDU Tactical Dark",
    "glyphs": "https://demotiles.maplibre.org/font/{fontstack}/{range}.pbf",
    "sources": {
        "openmaptiles": {
            "type": "vector",
            "url": "mbtiles://{path}"
        }
    },
    "layers": [
        {
            "id": "background",
            "type": "background",
            "paint": {
                "background-color": "#050505"
            }
        },
        {
            "id": "landcover-grass",
            "type": "fill",
            "source": "openmaptiles",
            "source-layer": "landcover",
            "filter": [
                "==",
                "class",
                "grass"
            ],
            "paint": {
                "fill-color": "#0f140f",
                "fill-opacity": 0.6
            }
        },
        {
            "id": "landcover-wood",
            "type": "fill",
            "source": "openmaptiles",
            "source-layer": "landcover",
            "filter": [
                "==",
                "class",
                "wood"
            ],
            "paint": {
                "fill-color": "#0a110a",
                "fill-opacity": 0.7
            }
        },
        {
            "id": "landuse-park",
            "type": "fill",
            "source": "openmaptiles",
            "source-layer": "landuse",
            "filter": [
                "in",
                "class",
                "park",
                "cemetery"
            ],
            "paint": {
                "fill-color": "#111611",
                "fill-opacity": 0.5
            }
        },
        {
            "id": "water",
            "type": "fill",
            "source": "openmaptiles",
            "source-layer": "water",
            "paint": {
                "fill-color": "#080c10"
            }
        },
        {
            "id": "waterway",
            "type": "line",
            "source": "openmaptiles",
            "source-layer": "waterway",
            "paint": {
                "line-color": "#1c2833",
                "line-width": 1.5
            }
        },
        {
            "id": "contour",
            "type": "line",
            "source": "openmaptiles",
            "source-layer": "contour",
            "paint": {
                "line-color": "#2a2a2a",
                "line-width": 0.5,
                "line-opacity": 0.6
            }
        },
        {
            "id": "contour-label",
            "type": "symbol",
            "source": "openmaptiles",
            "source-layer": "contour",
            "filter": [
                "all",
                [
                    ">",
                    "ele",
                    0
                ],
                [
                    "==",
                    [
                        "%",
                        "ele",
                        100
                    ],
                    0
                ]
            ],
            "layout": {
                "symbol-placement": "line",
                "text-field": "{ele}",
                "text-size": 10,
                "text-letter-spacing": 0.1
            },
            "paint": {
                "text-color": "#444444",
                "text-halo-color": "#000000",
                "text-halo-width": 1
            }
        },
        {
            "id": "building",
            "type": "fill",
            "source": "openmaptiles",
            "source-layer": "building",
            "paint": {
                "fill-color": "#1a1a1a",
                "fill-outline-color": "#222222"
            }
        },
        {
            "id": "road-path",
            "type": "line",
            "source": "openmaptiles",
            "source-layer": "transportation",
            "filter": [
                "==",
                "class",
                "path"
            ],
            "paint": {
                "line-color": "#444444",
                "line-width": 1,
                "line-dasharray": [
                    2,
                    2
                ]
            }
        },
        {
            "id": "road-track",
            "type": "line",
            "source": "openmaptiles",
            "source-layer": "transportation",
            "filter": [
                "==",
                "class",
                "track"
            ],
            "paint": {
                "line-color": "#555555",
                "line-width": 1.5,
                "line-dasharray": [
                    4,
                    2
                ]
            }
        },
        {
            "id": "road-minor",
            "type": "line",
            "source": "openmaptiles",
            "source-layer": "transportation",
            "filter": [
                "in",
                "class",
                "minor",
                "service"
            ],
            "paint": {
                "line-color": "#333333",
                "line-width": 2
            }
        },
        {
            "id": "road-secondary",
            "type": "line",
            "source": "openmaptiles",
            "source-layer": "transportation",
            "filter": [
                "==",
                "class",
                "secondary"
            ],
            "paint": {
                "line-color": "#555555",
                "line-width": 3
            }
        },
        {
            "id": "road-primary",
            "type": "line",
            "source": "openmaptiles",
            "source-layer": "transportation",
            "filter": [
                "==",
                "class",
                "primary"
            ],
            "paint": {
                "line-color": "#777777",
                "line-width": 4
            }
        },
        {
            "id": "boundary-state",
            "type": "line",
            "source": "openmaptiles",
            "source-layer": "boundary",
            "filter": [
                "==",
                "admin_level",
                4
            ],
            "paint": {
                "line-color": "#333333",
                "line-width": 1,
                "line-dasharray": [
                    3,
                    2
                ]
            }
        },
        {
            "id": "mountain-peak",
            "type": "symbol",
            "source": "openmaptiles",
            "source-layer": "poi",
            "filter": [
                "==",
                "class",
                "peak"
            ],
            "layout": {
                "icon-image": "mountain-11",
                "icon-size": 1,
                "text-field": [
                    "get",
                    "name"
                ],
                "text-size": 11,
                "text-offset": [
                    0,
                    1.2
                ],
                "text-anchor": "top"
            },
            "paint": {
                "text-color": "#aaaaaa",
                "text-halo-color": "#000000",
                "text-halo-width": 1.5
            }
        },
        {
            "id": "place-village",
            "type": "symbol",
            "source": "openmaptiles",
            "source-layer": "place",
            "filter": [
                "==",
                "class",
                "village"
            ],
            "layout": {
                "text-field": [
                    "get",
                    "name"
                ],
                "text-size": 12
            },
            "paint": {
                "text-color": "#999999",
                "text-halo-color": "#000000",
                "text-halo-width": 1.5
            }
        },
        {
            "id": "place-town",
            "type": "symbol",
            "source": "openmaptiles",
            "source-layer": "place",
            "filter": [
                "==",
                "class",
                "town"
            ],
            "layout": {
                "text-field": [
                    "get",
                    "name"
                ],
                "text-size": 14
            },
            "paint": {
                "text-color": "#bbbbbb",
                "text-halo-color": "#000000",
                "text-halo-width": 2
            }
        }
    ]
}